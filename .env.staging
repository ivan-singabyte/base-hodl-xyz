# Staging Environment Configuration
# Deploy to Vercel with these environment variables for staging

# OnchainKit API Key (can be same as production or separate)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_staging_onchainkit_api_key_here

# WalletConnect Project ID (can be same as production)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_walletconnect_project_id_here

# HODL Vault Contract Address on Base Sepolia
NEXT_PUBLIC_VAULT_ADDRESS=0x71Da6632aD3De77677E82202853889bFC5028989

# Staging Application URL (will be created as separate project)
NEXT_PUBLIC_URL=https://base-hodl-staging.vercel.app

# Network Configuration
NEXT_PUBLIC_CHAIN_ID=84532
NEXT_PUBLIC_CHAIN_NAME=Base Sepolia
NEXT_PUBLIC_IS_STAGING=true

# Optional: Separate analytics for staging
# NEXT_PUBLIC_ANALYTICS_ID=your_staging_analytics_id

# For contract deployment (only needed locally, not in Vercel)
# PRIVATE_KEY=your_private_key_here
# BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
# ETHERSCAN_API_KEY=your_etherscan_api_key